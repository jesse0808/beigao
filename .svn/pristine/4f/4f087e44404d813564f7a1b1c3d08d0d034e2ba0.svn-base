@font-face {
    font-family: 'AvantGarde LT ExtraLight';
    src: url("../font/AvantGardeLT-ExtraLight.woff2") format('woff2'),
         url('../font/AvantGardeLT-ExtraLight.woff2') format('woff');
    font-weight: normal;
    font-style: normal;
}

/* 淘宝代码初始化 */
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td {
    margin: 0;
    padding: 0;
}

/*body, button, input, select, textarea {*/
/*  font: 12px/1.5 tahoma, arial, \5b8b\4f53;*/
/*}*/
h1, h2, h3, h4, h5, h6 {
    font-size: 100%;
}

address, cite, dfn, em, var {
    font-style: normal;
}

code, kbd, pre, samp {
    font-family: couriernew, courier, monospace;
}

small {
    font-size: 12px;
}

ul, ol {
    list-style: none;
}

a {
    text-decoration: none;
}

sup {
    vertical-align: text-top;
}

sub {
    vertical-align: text-bottom;
}

legend {
    color: #000;
}

fieldset, img {
    border: 0;
}

button, input, select, textarea {
    outline: none;
    font-size: 100%;
}

table {
    border-collapse: collapse !important;
    border-spacing: 0;
}

body {
    width: 100%;
    font: 12px/1.5 "Microsoft YaHei", 微软雅黑, "MicrosoftJhengHei", 华文细黑, STHeiti, MingLiu;
    min-width: 1200px;
    padding: 0 !important;
}

* {
    font-stretch: condensed;
    box-sizing: border-box;
}

/* 全局加载中样式 */
.all_vue_loading {
    width: 100%;
    height: 100%;
}

[v-cloak] {
    display: none;
}

.errorInfos_alter_box {
    right: 50% !important;
    margin-right: -165px;
    top: 100px !important;
}

/* 最大盒子 */
.allgame-mainbox {
    position: relative;
    padding-bottom: 30px;
    overflow: hidden;
}

/* 中间内容框样式 */
.box_center {
    width: 1200px;
    margin: 0 auto;
    position: relative;
}

/* 清除浮动 */
.cf {
    zoom: 1;
}

.cf::before, .cf::after {
    display: table;
    content: '';
}

.cf::after {
    clear: both;
}

/* 设置弹性布局 */

/* 子元素纵向排列 */
.box_s {
    display: flex;
    flex-direction: column;
}

/* 子元素横向排列 */
.box_h {
    display: flex;
    flex-direction: row;
    box-orient: horizontal;
}

.box_flex {
    flex: 1;
}

/* 文字几行隐藏 */
.diandian {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.diandian2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.diandian3 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}

/* 定义所有节点为border-box */
* {
    box-sizing: border-box;
}

//element 样式
.el-input{

    &.is-focus{
        
        .el-input__inner{
            border-color: #2aaca2 !important;
        }
    }

    .el-input__inner{

        &:focus{
            border-color: #2aaca2 !important;
        }
    }
}

.el-select{

    .el-input__inner{
        &:focus{
            border-color: #2aaca2 !important;
        }
    }
}

.el-textarea{

    textarea{
        font-family: '微软雅黑';
        &:focus{
            border-color: #2aaca2 !important;
        }
    }
}

.el-select-dropdown{

    .el-select-dropdown__item{

        &.selected{
            color: #2aaca2;
        }
    }
}

//时间选择器
.el-date-picker{

    .el-date-picker__time-header{

        .el-time-panel__btn{

            &.confirm{
                color: #2aaca2;
            }
        }
        
        .el-date-picker__header-label{

            &:hover{
                color: #2aaca2;
            }
        }
    }

    .el-date-picker__header{

        .el-picker-panel__icon-btn,.el-date-picker__header-label{
            
            &:hover{
                color: #2aaca2;
            }
        }
    }

    .el-picker-panel__content{

        .el-date-table{

            td{
    
                &.current{
    
                    span{
                        background-color: #2aaca2;
                        color: #fff !important;
                    }
                    
                }

                &.today{

                    span{
                        color: #2aaca2;
                    }
                }

                &:hover{
                    color: #2aaca2;
                } 
            }
        }

        .el-year-table,.el-month-table{

            td{
                &.current,&.today,&:hover{
                    
                    a{
                        color: #2aaca2;
                    }
                }
            }
        }
    }
    
    .el-picker-panel__footer{

        .el-button--default{

            &:hover{
                color:#2aaca2;
                border: 1px solid #2aaca2;
            }
        }
        .el-button--text{
            color: #2aaca2;
        }
    }
}

//加载中样式
.el-loading-spinner {
    color: #2aaca2 !important;

    .el-icon-loading {
        color: #2aaca2 !important;
    }

    .el-loading-text {
        color: #2aaca2 !important;
    }

    .el-loading-mask {
        background: rgba(255, 255, 255, 0.8);
    }
}

/* 不同游戏玩法内容盒子 */
.gametype_center{
    width:100%;
    position: relative;
    box-shadow: 0px 2px 16px 0px rgba(53, 63, 75, 0.1);
    border-radius: 10px;
    padding-bottom: 10px;
    margin-top: 20px;

    .play_type{
        position: absolute;
        top: 0;
        left: -40px;
        width: 40px;
        height: 200px;
        border-radius: 10px;

        .playType_btn{
            width:100%;
            height: 100px;
            border-radius: 10px;
            font-size: 14px;

            &.playType_cur{
                background-color:#C19F62 ;

                a{
                    color: #fff;
                }
            }

            a{
                display: flex;
                justify-content: center;
                flex-direction: column;
                align-items: center;
                writing-mode:vertical-rl;
                width:100%;
                height: 100%;
                color: #333;
                transition: all .3s;

                &:hover{
                    opacity: .8;
                }
            }
        }
    }

    .official_type{
        position: absolute;
        top: 0;
        left: -40px;
        width: 40px;
        height: 100px;
        border-radius: 10px;
        display: flex;
        justify-content: center;
        flex-direction: column;
        align-items: center;
        writing-mode:vertical-rl;
        background-color:#C19F62 ;
        color: #fff;
        font-size: 14px;
        cursor: pointer;
    }
