    //状态颜色
    .unpaidState {
        color: #8b8b8b;
    }
    .chedan {
        color: #b0b0b0;
    }
    .cancelState {
        color: #008573;
    }

    .payState {
        color: #f21d32;
    }

    .pay_succ {
        color: #2ac1b1;
    }

    .pay_fail {
        color: #818181;
    }

    //搜索条件
    .query_info{

        .el-input,.el-select{
          width: 226px;
          margin-right: 12px;
        }

        .czCxBtn {
            width: 75px;
            height: 37px;
            background-color: #008573;
            border-radius: 5px;
            text-align: center;
            line-height: 37px;
            font-size: 14px;
            color: #fff;
            display: inline-block;
            cursor: pointer;
        }
    }
    //表格样式
    .czTable,.tableData{
        margin: 20px 0 0;

        .el-table{

            th,td{
                padding: 0;
                height: 35px;
                text-align: center;
                font-size: 12px;
                font-weight: normal;
            }

            th{
                font-size: 14px;
                text-align: center;
            }
        }

        .el-table__empty-block {
            display: none;
        }

        .detail_style{
            cursor: pointer;
            color: #008573;
        }
    }

    //表格数据为空样式
    .table-empty {
        padding-top: 80px;
        text-align: center;
  
        img{
            width: 102px;
            height: 78px;
        }
  
        .table-empty-text {
            font-size: 14px;
            margin-top: 20px;
            color: #8b918b;
        }
    }

    //分页样式
    .my-paging {
        padding: 12px 23px 0 20px;
  
        .everyPageShow {
            float: left;
            min-width: 84px;
            height: 27px;
            font-size: 12px;
            color: #67707c;
            line-height: 27px;
            text-align: center;
        }

        .totalNum {
            float: left;
            min-width:100px;
            height: 27px;
            line-height: 27px;
            font-size: 12px;
            margin-left: 10px;
          }

        .el-pagination {
            float: right;
            padding: 0;

            .el-pager{
                font-weight: normal;

                li{
                    &.active{
                        font-weight: 600;
                        color: #008573;
                    }

                    &:hover{
                        color: #008573;
                    }
                }
            }

            button{

                &:hover{
                    color: #008573;
                }
            }
        }

        .jumpPage {
            float: right;
            line-height: 28px;
            font-size: 14px;
            text-align: center;
            color: #008573;
            margin-left: 8px;
            cursor: pointer;
        }
    }
}    