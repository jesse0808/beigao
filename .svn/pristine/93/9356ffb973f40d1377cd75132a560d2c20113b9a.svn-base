    //追号
    .betting_frame_box_track{

        .bet_frame_mainbox_track{
            background-color: $baseColor;

            .track_num_main{
                background-color: $baseColor;

                .el-tabs--border-card{

                    .el-tabs__header{

                        .el-tabs__nav-scroll{

                            .el-tabs__item{
                                color: #171717 !important;
                            }
                        }
                    }

                    .el-tabs__content{
                        
                        .el-tab-pane{

                            .track_num_inputbox{

                                span{
                                    color: #707070;
                                }

                                input{
                                    border: solid 1px #e4e4e4;
                                }

                                button{
                                    border: solid 1px #e7e7e7;
                                    transition: all .3s;
                                    color: #666;

                                    &:hover{
                                        background-color:#e9e9e9;
                                    }
                                }
                            }
                        }

                    }
                }

                .track_tablebox{

                    .track_num_table{

                        thead{
                            background-color: #e5e5e5;
                        }

                        tbody{

                            tr{
                                border-bottom: 1px solid #eee;
                            }

                            .Multiple{
