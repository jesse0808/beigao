                    }
                }
            }
        }

        .lhc_ws{

            .lhc_ws_fun{

                .no_select_div{
                    color: #666;
                }
            }

            .lhc_ws_pick{
                background-color: rgba(231, 235, 246,.4);

            }
        }

        .lhc_zf{
            
            .zf_item{

                &.no_select_item{

                    b,div{
                        color: #666;
                    }

                    p{
                        color: #666;
                        background-color: rgba(231, 235, 246,.4);
                    }
                }
                
                &.select_item{

                    p{
                        background-color: rgba(231, 235, 246,.4);
                    }
                }
            }
        }

        .lhc_bz{
            background-color: rgba(231, 235, 246,.4);

            .bz_item{

                p{
                    color: #666;
                }
            }
        }

        .tema{

            .fun_area{
    
                .class_item{
                    border-right: 1px solid #C6D1D0;
                }
        
                .class_sx{
                    border-right: 1px solid #C6D1D0;
        
                    .no_select_sx{
                        color: #666;
                    }
                }
        
                .class_three{
        
                    .no_select_sx{
                        color: #666;
                    }
                }
            }
        
            .pick_area{
                background-color: rgba(231, 235, 246,.4);
    
                .no_select_numerical{
        
                    p{
                        color: #666;
                    }
                }
            }
        }
    
        .sum_item{
    
            .sum_box{
    
                .sum_money{
    
                    label,p,input{
                        color: #333;
                    }
                }  
            }
        }
    }

    //直选和值
    .gamecount{

        .twenty_list_div{
            background-color:rgba(231, 235, 246, 0.4);

            .twenty_item{

                .list_item_bg{
                    background-image: linear-gradient(0deg, #f2f2f2 0%, #ffffff 100%), linear-gradient( #ececec, #ececec);
                }
            }
        }
    }

    //投注金额
    .user_pay{

        .quick_user_inputbox{
            
            .comput_flod{

                button{
                    color: #999;
                }

                div{

                    input{
                        color: #000;
                    }
                }
            }

            .quick_user_input1,.quick_user_input2{

                span{
                    color: #333;
                }
            }

            .quick_user_input2{

                div{
                    color: #999;
                }
            }
        }
    }

    //开奖记录
    .lottery_periods{

        .lottery_periods_title{
            color: #333;
            background-color:$titleColor;

            a{
                color: #333;
            }
        }

        .lottery_periods_content{

            .el-row-title{
                color: #666;
            }

            .lottery_periods_item{

                &:nth-child(odd){
                    background-color: $baseColor;
                }

                &:nth-child(even){
                    background-color: $titleColor;
                }
            }

            .lottery_lhc_item{

                .lhc_title{
                    color: #666;
                }

                .lhc_content{
                    color: #666;

                    span,strong{
                        color: #666;
                    }

                    .lhc_content_item{

                        div{

                            &:last-child{
                                color: #666;
                            }
                        }
                    }
                }
            }
        }
    }

    //投注与追号记录
    .gametype_table_mainbox{
        background-color: $baseColor;

        .gametype_table_nav{

            .gametype_title{
                background-color: $titleColor;

                .gametype_title_btn{

                    a{
                        color: #333;
                    }
                }
            }

            .gametype_getmore_box{

                a{
                    color: #333;
                }
            }
        }

        .recordtable_bg_box{

            .gametype_table_main{

                .el-table{

                    .el-table__header-wrapper{
                        background-color: $titleColor;

                        thead{

                            th{
                                .cell{
                                    color: #232924;
                                }
    
                            }
                        }  
                    }
                    
                    .el-table__body-wrapper{

                        tbody{

                            tr{
                                td{
                                    border-bottom: 1px solid #e3e3e4 !important;
                                }
                            }
                        }
                    }
                }

            }
        }
    }

    //投注提示
    .betting_frame_box
    {
