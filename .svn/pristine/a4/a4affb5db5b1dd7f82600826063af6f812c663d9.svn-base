.recing_content_left > p {
    position: relative;
    color: #ffffff;
    padding: 5px 12px;
    font-size: 12px;
    background-color: #008573;
    border-radius: 3px;
    box-shadow: inset 1px 2px 3px 0 rgba(0, 0, 0, 0.43);
}

.recing_content_left > p:after {
    content: '';
    position: absolute;
    right: -10px;
    top: 0;
    bottom: 0;
    margin: auto;
    width: 0;
    height: 0;
    border-left: 5px solid #008573;
    border-top: 5px solid transparent;
    border-bottom: 5px solid transparent;
    border-right: 5px solid transparent;
}

