.big_icon {
    animation: bigIcon .3s ease both;;
}

@keyframes bigIcon {
    0% {
        -webkit-transform: scale(1);
    }
    50% {
        -webkit-transform: scale(1.2);
    }
    100% {
        -webkit-transform: scale(1);
    }
}

